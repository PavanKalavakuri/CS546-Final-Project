<section class="homesearchsec">
  <div class="container">
    <div class="row">
      <div class="col py-5">
        <h1 class="home_text">Find the right Doctors</h1>
        <h1 class="home_text">Who accept your Insurance</h1>
        <div class="mt-3">
          {{> searchbar}}
        </div>
      </div>
    </div>
  </div>
</section>

<section class="homewhitesec">
  <div class="container">
    <div class="row">
      <div class="col">
        <div>
          <h2>
            Top Rated Doctors
          </h2>
        </div>
        <div
          id="carouselExampleDark1"
          class="carousel carousel-dark slide"
          data-bs-ride="carousel"
        >
          <div class="carousel-inner">
            <div class="carousel-item active" data-bs-interval="10000">
              <div class="row">
                {{#each docs1}}
                  <div class="col">
                    <div class="card card-body h-100 rounded-0">
                      <div class="row">
                        <div class="col-4">
                          <img
                            class="img-thumbnail border-secondary rounded-circle"
                            src="/public/img/{{this._id}}.png"
                            alt="doctor"
                          />
                        </div>
                        <div class="col">
                          <p class="fw-bold fs-4">Dr.
                            {{this.firstname}}
                            {{this.lastname}},
                            {{this.qualification}}</p>
                          <p>{{this.specialty}}</p>
                          <p class="text-muted">{{this.city}},
                            {{this.state}}</p>
                          <ul class="ps-0">
                            <li class="list-inline-item mt-3"><i
                                class="fa fa-star star me-2"
                                aria-hidden="true"
                              ></i>{{this.rating}}</li>
                            <li class="text-right list-inline-item"><a
                                class="nav-link stretched-link"
                                href="/doctor/{{this._id}}"
                              >View Availability</a></li>
                          </ul>
                        </div>
                      </div>
                    </div>
                  </div>
                {{/each}}
              </div>
            </div>
            <div class="carousel-item">
              <div class="row">
                {{#each docs2}}
                  <div class="col">
                    <div class="card card-body h-100 rounded-0">
                      <div class="row">
                        <div class="col-4">
                          <img
                            class="img-thumbnail border-secondary rounded-circle"
                            src="/public/img/{{this._id}}.png"
                            alt="doctor"
                          />
                        </div>
                        <div class="col">
                          <p class="fw-bold fs-4">Dr.
                            {{this.firstname}}
                            {{this.lastname}},
                            {{this.qualification}}</p>
                          <p>{{this.specialty}}</p>
                          <p class="text-muted">{{this.city}},
                            {{this.state}}</p>
                          <ul class="ps-0">
                            <li class="list-inline-item mt-3"><i
                                class="fa fa-star star me-2"
                                aria-hidden="true"
                              ></i>{{this.rating}}</li>
                            <li class="text-right list-inline-item"><a
                                class="nav-link stretched-link"
                                href="/doctor/{{this._id}}"
                              >View Availability</a></li>
                          </ul>
                        </div>
                      </div>
                    </div>
                  </div>
                {{/each}}
              </div>
            </div>
            <div class="carousel-item">
              <div class="row">
                {{#each docs3}}
                  <div class="col">
                    <div class="card card-body h-100 rounded-0">
                      <div class="row">
                        <div class="col-4">
                          <img
                            class="img-thumbnail border-secondary rounded-circle"
                            src="/public/img/{{this._id}}.png"
                            alt="doctor"
                          />
                        </div>
                        <div class="col">
                          <p class="fw-bold fs-4">Dr.
                            {{this.firstname}}
                            {{this.lastname}},
                            {{this.qualification}}</p>
                          <p>{{this.specialty}}</p>
                          <p class="text-muted">{{this.city}},
                            {{this.state}}</p>
                          <ul class="ps-0">
                            <li class="list-inline-item mt-3"><i
                                class="fa fa-star star me-2"
                                aria-hidden="true"
                              ></i>{{this.rating}}</li>
                            <li class="text-right list-inline-item"><a
                                class="nav-link stretched-link"
                                href="/doctor/{{this._id}}"
                              >View Availability</a></li>
                          </ul>
                        </div>
                      </div>
                    </div>
                  </div>
                {{/each}}
              </div>
            </div>
          </div>
          <button
            class="carousel-control-prev"
            type="button"
            data-bs-target="#carouselExampleDark1"
            data-bs-slide="prev"
          >
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
          </button>
          <button
            class="carousel-control-next"
            type="button"
            data-bs-target="#carouselExampleDark1"
            data-bs-slide="next"
          >
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
        </div>
      </div>
    </div>
  </div>
</section>

