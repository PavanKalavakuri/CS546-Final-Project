  <link rel="stylesheet" href="/public/css/home.css">
  {{/if}}
  {{#if error}}
  <link rel="stylesheet" href="/public/css/error.css" />
  {{/if}}
</head>

<body>
  <header>
    <div id="nav">
    <nav class="navbar navbar-expand navbar-light bg-white fixed-top border-bottom">
      <div class="container-fluid">
        <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#main-nav"
          aria-controls="main-nav" aria-expanded="false" aria-label="Toggle navigation">
          <i class="fas fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse" id="main-nav">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item active">
              <a class="nav-link" aria-current="page" href="/">Home</a>
            </li>
            <li class="nav-item active">
              <a class="nav-link" aria-current="page" href="/search">Search</a>
            </li> 
            {{! <li class="nav-item">
              <a class="nav-link" href="#">About</a>
            </li> }}
          </ul>
          <ul>
          <li class="d-flex nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="#" id="navbarScrollingDropdown" role="button"
              data-bs-toggle="dropdown" aria-expanded="false">
              {{#if auth}}
              {{auth.firstname}}
              {{else}}
              Login / Register
              {{/if}}
            </a>
            <ul class="dropdown-menu" aria-labelledby="navbarScrollingDropdown">
              <li>
                {{#if auth}}
                <a class="dropdown-item" href="/{{auth.type}}/home">My
                  Account</a>
                {{else}}
                <a class="dropdown-item" href="/user/login">Patient</a>
                {{/if}}
              </li>
              <li>
                <hr class="dropdown-divider" />
              </li>
              <li>
                {{#if auth}}
                <a class="dropdown-item" href="/logout">Logout</a>
                {{else}}
                <a class="dropdown-item" href="/doctor/login">Doctor</a>
                {{/if}}
              </li>
            </ul>
          </li>
          </ul>
        </div>
      </div>
    </nav>
    </div>
  </header>
  {{{body}}}
  <footer>

  </footer>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  {{#if script_file}}
  <script src="/public/js/{{script_file}}.js"></script>
  {{/if}}
  <script src="https://cdn.jsdelivr.net/npm/moment@2.29.3/moment.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
